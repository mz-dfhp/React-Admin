{
  "name": "mz-react",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "lint": "npm run lint:eslint & npm run lint:prettier",
    "lint:eslint": "eslint --fix ./src/**/*.{js,jsx,ts,tsx,vue} --cache --cache-location node_modules/.cache/eslint/",
    "lint:prettier": "prettier --write ./src/**/*.{js,ts,json,tsx,css,less,scss,vue,html,md} --loglevel warn --cache",
    "lint:lint-staged": "lint-staged",
    "preinstall": "npx only-allow pnpm",
    "postinstall": "simple-git-hooks"
  },
  "simple-git-hooks": {
    "pre-commit": "npx lint-staged",
    "commit-msg": "npx --no-install commitlint -e"
  },
  "dependencies": {
    "@ant-design/icons": "^4.8.0",
    "@redux-devtools/extension": "^3.2.3",
    "antd": "^5.0.3",
    "lodash-es": "^4.17.21",
    "normalize.css": "^8.0.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-redux": "^8.0.5",
    "react-router-dom": "^6.4.4",
    "redux": "^4.2.0",
    "redux-persist": "^6.0.0",
    "redux-thunk": "^2.4.2"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.2.0",
    "@commitlint/config-conventional": "^17.2.0",
    "@iconify-json/ic": "^1.1.13",
    "@iconify-json/zondicons": "^1.1.4",
    "@types/lodash-es": "^4.17.6",
    "@types/node": "^18.11.10",
    "@types/react": "^18.0.22",
    "@types/react-dom": "^18.0.7",
    "@typescript-eslint/eslint-plugin": "^5.42.0",
    "@typescript-eslint/parser": "^5.42.0",
    "@vitejs/plugin-react": "^2.2.0",
    "eslint": "^8.26.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.31.10",
    "lint-staged": "^13.0.3",
    "prettier": "^2.7.1",
    "simple-git-hooks": "^2.8.1",
    "typescript": "^4.6.4",
    "unocss": "^0.47.4",
    "vite": "^3.2.0",
    "vite-plugin-eslint": "^1.8.1"
  }
}
